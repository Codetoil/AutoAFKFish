{
  "schemaVersion": 1,
  "id": "autoafkfish",
  "version": "1.1.3",

  "name": "AutoAFKFish",
  "description": "A mod that lets you AFK Fish in the background when used with an auto fishing setup.\nDefault Keybind is F12.",
  "authors": [
    "Codetoil"
  ],
  "contributors": [
    "Calvineries",
    "Wudji"
  ],
  "contact": {
    "homepage": "https://codetoil.github.io/",
    "sources": "https://github.com/Codetoil/AutoAFKFish"
  },

  "license": "GNU-GPL-v3",
  "icon": "assets/icon.png",

  "environment": "*",
  "entrypoints": {
    "client": [
      "io.codetoil.autoafkfish.AutoAFKFish"
    ]
  },
  "mixins": [
    {
      "config": "autoafkfish.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.16.14",
    "minecraft": ">1.21.8",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
    "flamingo": "*",
    "modmenu": "*",
    "modupdater": "*"
  },
  "custom": {
    "modmenu:clientsideOnly": true,
    "modupdater": {
      "strategy": "curseforge",
      "projectID": 358814
    }
  }
}
